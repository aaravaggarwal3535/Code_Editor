{
  "name": "code-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:backend": "node backend/server.js",
    "start:dev": "concurrently \"npm run dev\" \"npm run start:backend\"",
    "start": "concurrently \"npm run preview\" \"npm run start:backend\"",
    "start:ai": "node backend/simple-ai-service.js",
    "start:all": "concurrently --names \"BACKEND,AI,FRONTEND\" --prefix-colors \"yellow,blue,green\" --kill-others \"npm run start:backend\" \"npm run start:ai\" \"npm run dev\"",
    "start:app": "npm run start:all"
  },
  "dependencies": {
    "groq-sdk": "^0.19.0",
    "@monaco-editor/react": "^4.7.0",
    "axios": "^1.8.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  }
}
